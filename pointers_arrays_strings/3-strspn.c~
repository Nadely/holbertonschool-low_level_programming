#include "main.h"
#include "2-strlen.c"
/**
 * _strspn - prefix
 * Return: prefix
 * @s: pointer
 * @accept: pointer
 */
unsigned int _strspn(char *s, char *accept)
{
	int c;
	accept = _strlen + 1;
	if (accept == NULL)
	{
		exit (1);
	}
	for (c = 0; c < _strlen; c++)
	{
		*(accept + c) = (*(s + accept - 1);
				 s++;
				 }
			*(accept + c) = '\0';
		return (accept);
	}
