#include "main.h"
#include <stdlib.h>
/**
 * create_array - array of chars
 * Return: array of chars
 * @size: size
 * @c: characters
 */
char *create_array(unsigned int size, char c)
{
	for (size = malloc(c))
	{
	if (size != 0)
		return(*size);
	if (size == 0 || NULL)
                return(NULL);
	}
	return(0);
}
